          <con4:route>
            <con2:id>_ActionId-2214840675653925852--26d5029e.14881ef0fd3.-79f9</con2:id>
            <con4:service ref="task1/proxy/ProxyService3" xsi:type="ref:ProxyRef" xmlns:ref="http://www.bea.com/wli/sb/reference"/>
            <con4:outboundTransform/>
