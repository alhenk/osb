            <con2:id>_ActionId-84981398926510399-1649bfaa.1488befb0b9.-7e54</con2:id>
            <con3:logLevel>warning</con3:logLevel>
            <con3:expr>
              <con2:xqueryText>$body</con2:xqueryText>
            </con3:expr>
            <con3:message>&lt;&lt;THE INFO WAS SENT TO PROXY3>></con3:message>
